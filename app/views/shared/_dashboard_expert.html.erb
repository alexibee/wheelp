<div class="container">

  <%= link_to "Create a service", new_service_path, class: "grey-button" %>
  <% if @message %>
    <h3> <%= @message %> </h3>
  <% else %>
    <h2> Evaluation requests: </h2>
    <% @bookings.each do |booking| %>
        <%= render "shared/booking_dashboard_card_expert", booking: booking %>
    <% end %>
  <% end %>
</div>
