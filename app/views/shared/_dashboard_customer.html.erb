<% if @message %>
  <h3> <%= @message %> </h3>
  <%= link_to "Find an expert", services_path, class: "link" %>
<% else %>
  <% @bookings.each do |booking| %>
    <%= link_to booking.vehicle_address, class: "link" %>
  <% end %>
  <%= link_to "Find an expert", services_path, class: "link" %>
<% end %>
