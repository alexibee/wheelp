<div class="container col-6 mt-10 form">
  <h3>Create a new service</h3>
  <%= simple_form_for [ @service ] do |f| %>
    <%= f.input :title %>
    <%= f.input :address %>
    <%= f.input :bio, as: :text %>
    <%= f.input :price, as: :numeric %>
    <%= f.input :photo, as: :file %>
    <%= f.submit "Create service", class: "btn" %>
  <% end %>
</div>
