
<h1> Your booking </h1>
<div>
  <% simple_form_for(@booking) do |f| %>
    <%= f.input :vehicle_brand %>
    <%= f.input :vehicle_model %>
    <%= f.input :vehicle_address %>
    <%= f.input :vehicle_year %>
    <%= f.input :vehicle_contact %>
    <%= f.input :additional_details %>
    <%= f.input :date, as: :date, html5: true  %>
  <% end %>
</div>
