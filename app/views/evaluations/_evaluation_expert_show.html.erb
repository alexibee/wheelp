<div class="container col-6 text-center">
  <h1> Your evaluation </h1>
  <p>"<%= evaluation.comment %>"</p>
  <p> <%= cl_image_tag evaluation.photo.key if evaluation.photo.attached?%> </p>
  <p> <%= cl_video_tag evaluation.video.key, controls: true  if evaluation.video.attached?%> </p>
  <p> <%= cl_image_path evaluation.document.key if evaluation.document.attached?%> </p>
    <%= link_to "Back", service_booking_path(booking.service, booking) %>
  </p>
  <p>
    <%= link_to "Back to your dashboard", dashboard_path %>
  </p>
</div>
